{
  "name": "reactjs-mentoring",
  "version": "1.0.0",
  "description": "reactjs-mentoring-2021",
  "main": "index.js",
  "scripts": {
    "dev": "webpack --mode development",
    "build": "webpack --mode production",
    "watch": "webpack --mode development --watch",
    "start": "webpack serve --mode development --open",
    "lint": "eslint ./src",
    "prettier": "prettier",
    "test": "jest",
    "precommit": "lint-staged"
  },
  "browserslist": "> 0.25%, not dead",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Aniuta907/ReactJS-Mentoring.git"
  },
  "keywords": [
    "js",
    "javascript",
    "webpack",
    "react"
  ],
  "author": "Anna <anna.kuznetsova111@yandex.ru>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Aniuta907/ReactJS-Mentoring/issues"
  },
  "homepage": "https://github.com/Aniuta907/ReactJS-Mentoring#readme",
  "lint-staged": {
    "*.+(ts|tsx)": [
      "eslint ./src --fix",
      "git add"
    ],
    "*.+(ts|tsx|json|scss|md)": [
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "preset": "ts-jest",
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "moduleNameMapper": {
      "\\.(css|less|scss|sss|styl)$": "<rootDir>/node_modules/jest-css-modules"
    },
    "globals": {
      "ts-jest": {
        "compiler": "ttypescript"
      }
    },
    "verbose": true,
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "setupFiles": [
      "<rootDir>/setupTests.ts"
    ],
    "snapshotSerializers": ["enzyme-to-json/serializer"]
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/plugin-proposal-class-properties": "^7.12.13",
